package entry;

import java.util.ArrayList;

public class Entry {
	public static void main(String[] args){
ArrayList<Integer> list = new ArrayList<Integer>();
		
		for(int i = 0; i < 100; i++)
			list.add(i);
		
		System.out.println(list.size());
		
		while(list.iterator().hasNext()){
			int item = list.iterator().next();
			if(item % 2 == 0){
				list.remove(item);
			}
		}
		
		System.out.println(list.size());
	}
}
