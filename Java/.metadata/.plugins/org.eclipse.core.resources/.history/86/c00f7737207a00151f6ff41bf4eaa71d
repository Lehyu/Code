package com.tool;

import java.io.File;
import java.io.FileInputStream;
import java.io.InputStream;

public class fileTool {
    private InputStream stream;
    private String filename;

    public fileTool(String name){
        filename = name;
        try{
            stream = new FileInputStream(new File(filename));
        }catch(Exception e){

        }
    }
}
