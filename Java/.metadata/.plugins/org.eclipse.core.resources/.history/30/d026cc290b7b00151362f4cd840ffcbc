package com.entry;

import java.io.IOException;
import java.util.HashMap;

import com.Algorithm.Bayesian;
import com.tool.FileIOTool;

public class DMPro {
	private static final String filename = "/home/lhy/Code/Java/DMPro/data/data.txt";
	public static void main(String[] args){
		FileIOTool tool = new FileIOTool(filename);
		try {
			String[][] data = tool.read();
			for(int i = 0; i < data.length; i++){
				for(int j = 0; j < data[i].length; j++)
					System.out.print(data[i][j]+" ");
				System.out.println();
				
			}
			HashMap<Integer,String> condition = new HashMap();
	    	condition.put(0, "sunny");
	    	condition.put(1, "85");
	    	condition.put(2, "85");
	    	condition.put(3, "FALSE");
	    	condition.put(4, "no");
	    	System.out.println(new Bayesian().selected(data, condition));
	    	String str = ">=80";
	    	String[] tmp = str.split(">=|<=|!=|==|>|<");
	    	String value = tmp[1];
	    	tmp = str.split(value);
	    	String exp = tmp[0];
	    	System.out.println(exp);
	    	System.out.println(value);
	    	System.out.println(str.replaceAll(">=|<=|!=|==|>|<",""));
		} catch (IOException e) {
			// TODO Auto-generated catch block
			e.printStackTrace();
		}
	}
}
