package model;

import java.util.Comparator;

/**
 * Created by lhy on 10/23/15.
 */
public class BagComparetorBySize<Item> implements Comparator<Bag<Item>>{
	@Override
    public int compare(Bag<Item> lhs, Bag<Item> rhs) {
        return ((Bag<Item>)lhs).size() > ((Bag<Item>)rhs).size() ? 1: 0;
    }
}
